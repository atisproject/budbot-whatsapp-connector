# BudBot WhatsApp Connector - Configurações de Ambiente

# Porta do servidor (Render define automaticamente)
PORT=3000

# URL do backend principal
BACKEND_URL=https://budbot-ia.onrender.com

# Token de segurança para webhook
WEBHOOK_TOKEN=budbot_webhook_secret_2025

# Configurações do WhatsApp Web Session
WWEB_SESSION_PATH=./wweb_session

# Configurações do Puppeteer (Render compatível)
PUPPETEER_EXECUTABLE_PATH=

# Modo de desenvolvimento
NODE_ENV=production

# Configurações de logging
LOG_LEVEL=info